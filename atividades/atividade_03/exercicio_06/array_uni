#include <stdio.h>

int main() {
    int numeros[5]; 
    int i, soma = 0;

   
    printf("Digite 5 números inteiros:\n");
    for (i = 0; i < 5; i++) {
        printf("Número %d: ", i + 1);
        scanf("%d", &numeros[i]);
    }

   
    for (i = 0; i < 5; i++) {
        soma += numeros[i];
    }

   
    printf("A soma dos números é: %d\n", soma);

    return 0;
}
