#include <stdio.h>

int main() {
    int numeros[5]; // array para armazenar os 5 números inteiros
    int i, soma = 0; // variável para armazenar a soma

    // ler os 5 números inteiros do usuário
    printf("Digite 5 números inteiros:\n");
    for (i = 0; i < 5; i++) {
        printf("Número %d: ", i + 1);
        scanf("%d", &numeros[i]);
    }

    // calcular a soma dos números
    for (i = 0; i < 5; i++) {
        soma += numeros[i];
    }

    // exibir a soma
    printf("A soma dos números é: %d\n", soma);

    return 0;
}