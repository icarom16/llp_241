#include <stdio.h>

int main() {
    int matriz[3][3]; 
    int i, soma = 0; 

    
    printf("Digite a matriz 3x3:\n");
    for (i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            printf("Elemento [%d][%d]: ", i, j);
            scanf("%d", &matriz[i][j]);
        }
    }

   
    for (i = 0; i < 3; i++) {
        soma += matriz[i][i];
    }

   
    printf("A soma dos elementos da diagonal principal Ã©: %d\n", soma);

    return 0;
}
